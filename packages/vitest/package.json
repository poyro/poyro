{
  "name": "@poyro/vitest",
  "version": "0.5.0",
  "license": "MIT",
  "type": "module",
  "sideEffects": false,
  "main": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "files": [
    "dist/**",
    "constants.json"
  ],
  "exports": {
    ".": {
      "import": {
        "types": "./dist/index.d.ts",
        "default": "./dist/index.js"
      }
    },
    "./*": "./*",
    "./config": {
      "types": "./dist/config.d.ts",
      "default": "./dist/config.js"
    },
    "./fn": {
      "types": "./dist/functions/index.d.ts",
      "default": "./dist/functions/index.js"
    }
  },
  "engines": {
    "node": ">=20.0.0"
  },
  "scripts": {
    "build": "tsup",
    "clean": "rm -rf dist",
    "dev": "tsup --watch",
    "typecheck": "tsc --noEmit",
    "lint": "eslint src/",
    "test": "vitest --pool forks --no-file-parallelism --reporter verbose",
    "test:ci": "CI=true vitest"
  },
  "devDependencies": {
    "@repo/eslint-config": "workspace:*",
    "@repo/typescript-config": "workspace:*",
    "@types/node": "^20.14.5",
    "@vitest/expect": "^1.6.0",
    "tsup": "^8.1.0",
    "typescript": "^5.4.5"
  },
  "peerDependencies": {
    "vitest": ">= 1.6.0"
  },
  "dependencies": {
    "chalk": "^5.3.0",
    "find-cache-dir": "^5.0.0",
    "handlebars": "^4.7.8",
    "node-fetch": "^3.3.2",
    "node-llama-cpp": "3.0.0-beta.38",
    "radash": "^12.1.0",
    "yup": "^1.4.0"
  }
}
